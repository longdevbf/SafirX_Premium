{
  "builds": [
    {
      "src": "src/listener/index.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "src/listener/keepAlive.ts"
    },
    {
      "src": "/ping", 
      "dest": "src/listener/keepAlive.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/listener/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
